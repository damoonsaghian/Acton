	https://www.lua.org/pil/7.4.html
	https://www.lua.org/pil/7.5.html
	https://www.lua.org/pil/9.3.html

Iter[X]' :=
	index: Num'u,
	poll: { index :Num'u | X? }

;ns Iter[X]
	{ poll &{ index :Num'u | X? } | :Iter[X]
		index: 0, poll
	}
	
	each: { self, do ${elem :X} |
		;c
		elem = self.poll(self..index + 1)
		;c
	}
